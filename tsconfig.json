{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "types": [
      "node",
      "webdriverio",
      "webdriverio/globals",
      "jasmine"
    ]
  },
  "include": [
    "src",
    "utils",
    "test",
    "**/*.ts"
  ]
}